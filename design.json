      "contextualStyling": {
        "rules": [
          "Navigation containers use solid brand color (#7D7CF5) for active items; content cards keep solid white surfaces.",
          "Primary call-to-action in side panels uses dark solid (#111827) with white text; secondary utility buttons use white with gray border.",
          "System icons (navigation, actions) inherit neutral grays; decorative illustrations stay within cards and never adopt brand fills.",
          "Do not apply card shadows to inner content; shadows belong to outer containers only.",
          "Typography color hierarchy: headings #111827, body #374151, secondary #6B7280, tertiary/meta #9CA3AF."
        ],
        "hierarchy": {
          "parents": {
            "card": { "childrenInheritRadius": true, "childrenInheritShadow": false },
            "sidebar": { "itemsInheritTypography": true }
          },
          "overrides": {
            "badgeInsideCard": "Badge surface overrides card background to tokenized semantic surface.",
            "activeNavItem": "Text/icon colors override inherited neutral values to inverse on brand."
          },
          "responsive": {
            "≤768": {
              "gridGutter": 12,
              "cardRadius": 14,
              "shadowReduce": "0 4px 14px rgba(17,24,39,0.06)"
            }
          },
          "stateDependent": {
            "focusVisible": "Show 3px focus ring using brand or dark neutral depending on control style.",
            "disabled": "Reduce opacity to 60% and set cursor to not-allowed; never change layout."
          }
        }
